<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{
    @foreach (var job in ViewBag.jobs)
    {
        <table class="job-listing">
            @foreach (KeyValuePair<string, string> column in job)
            {
                <tr>
                    <td>@column.Key</td>
                    <td>@column.Value</td>
                </tr>

            }
        </table>

    }
    // TODO #3 - Use the same list display from Views/Search/Index.cshtml
}